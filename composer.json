{
    "name": "busbyjon/blog",
    "description": "Jon Busby's Wordpress Blog",
    "repositories":[
        {
            "type":"composer",
            "url":"http://wpackagist.org"
        },
        {
            "type": "git",
            "url":  "https://github.com/busbyjon/jonbusby-blog.git"
        }
    ],
    "require": {
        "ext-memcached": "*",
        "ext-apcu":"*",
        "johnpbloch/wordpress": "4.3",
        "wpackagist-plugin/akismet":"dev-trunk",
        "wpackagist-plugin/captcha":">=4.1.3",
        "wpackagist-plugin/wordpress-importer":"0.6.1",
        "wpackagist-plugin/w3-total-cache":"0.9.4.1",
	    "wpackagist-plugin/broken-link-checker":"1.10.9",
        "busbyjon/jonbusby-blog": "dev-master",
        "wpackagist-plugin/filebrowser": "dev-trunk"
    },
    "extra": {
       "heroku": {
            "nginx-includes": ["nginx.conf"]
        }
    },
    "scripts": {
    }

}
